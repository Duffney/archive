              memorystartupbytes="1GB"
              dynamicmemoryenabled="Y"
              processorcount="1"
              administratorpassword="P@ssw0rd"
              productkey="HJNDF-8D67J-BV24J-4BH9F-GXWFY"
              timezone="Central Standard Time"
              ostype="Server" />              
    <template name="Windows Server 2012 R2 Datacenter Core"

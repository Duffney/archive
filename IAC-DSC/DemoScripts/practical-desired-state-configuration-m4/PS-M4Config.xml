<vms>
       <vm name="PSM4-DC"
        template="Windows Server 2012 R2 PSv5"
        computername="DC"
        memorystartupbytes="2GB"
        dynamicmemoryenabled="N">
      <dsc configname="DC_FORESTPRIMARY"
           configfile="DC_FORESTPRIMARY.DSC.ps1">
        <parameters>
          DomainName = "GLOBOMANTICS.COM"
          DomainAdminPassword = "P@ssw0rd"
          Forwarders = @('8.8.8.8','8.8.4.4')
        </parameters>
      </dsc>
      <adapters>
        <adapter name="Internal"
                 switchname="Internal">
          <ipv4 address="192.168.2.10"
                defaultgateway="192.168.2.1"
                subnetmask="24"
                dnsserver="192.168.2.10"/>
          <ipv6 address="fd53:ccc5:895a:bc00::a"
                defaultgateway="fd53:ccc5:895a:bc00::13"
                subnetmask="64"
                dnsserver="fd53:ccc5:895a:bc00::a"/>
        </adapter>
      </adapters>              
    </vm>
    <vm name="PSM4-Cert"
        template="Windows Server 2012 R2 PSv5"
        computername="Cert"
        memorystartupbytes="2GB"
        dynamicmemoryenabled="N">
      <dsc configname="MEMBER_ROOTCA"
           configfile="MEMBER_ROOTCA.DSC.ps1">
        <parameters>
          DomainName = "GLOBOMANTICS.COM"
          DomainAdminPassword = "P@ssw0rd"
          DCName = 'DC'
          PSDscAllowDomainUser = $True
          InstallRSATTools = $True
        </parameters>
      </dsc>            
      <adapters>
        <adapter name="Internal"
                 switchname="Internal">
        </adapter>
      </adapters>              
    </vm>      
    <vm name="PSM4-DHCP"
        template="Windows Server 2012 R2 PSv5"
        computername="DHCP">
      <dsc configname="MEMBER_DHCP"
           configfile="MEMBER_DHCP.DSC.ps1">
        <parameters>
          DomainName = "GLOBOMANTICS.COM"
          DCname = "DC"
          DomainAdminPassword = "P@ssw0rd"
          PSDscAllowDomainUser = $True
          Scopes = @(
              @{ Name = 'Site A Primary';
                 Start = '192.168.2.50';
                 End = '192.168.2.254';
                 SubnetMask = '255.255.255.0';
                 AddressFamily = 'IPv4'
              }
            )
          Reservations = @(
              @{ Name = 'DC';
                 ScopeID = '192.168.2.0';
                 ClientMACAddress = '000000000000';
                 IPAddress = '192.168.2.10';
                 AddressFamily = 'IPv4'
              },
              @{ Name = 'DHCP';
                 ScopeID = '192.168.2.0';
                 ClientMACAddress = '000000000002';
                 IPAddress = '192.168.2.16';
                 AddressFamily = 'IPv4'
              }
            )
          ScopeOptions = @(
              @{ ScopeID = '192.168.2.0';
                 DNServerIPAddress = @('192.168.2.10');
                 Router = '192.168.2.1';
                 AddressFamily = 'IPv4'
              }
            )
        </parameters>
      </dsc>
      <adapters>
        <adapter name="Internal"
                 switchname="Internal">
          <ipv4 address="192.168.2.16"
                defaultgateway="192.168.2.1"
                subnetmask="24"
                dnsserver="192.168.2.10"/>
          <ipv6 address="fd53:ccc5:895a:bc00::10"
                defaultgateway=""
                subnetmask="64"
                dnsserver="fd53:ccc5:895a:bc00::a"/>
        </adapter>
      </adapters>
    </vm> 
    <vm name="PSM4-S2"
        template="Windows Server 2012 R2 PSv5"
        computername="S2"
        memorystartupbytes="2GB"
        dynamicmemoryenabled="N">
      <dsc configname="MEMBER_DEFAULT"
           configfile="MEMBER_DEFAULT.DSC.ps1">
        <parameters>
          DomainName = "GLOBOMANTICS.COM"
          DomainAdminPassword = "P@ssw0rd"
          DCName = 'DC'
          PSDscAllowDomainUser = $True
        </parameters>
      </dsc>           
      <adapters>
        <adapter name="Internal"
                 switchname="Internal">
        </adapter>
      </adapters>              
    </vm>
    <vm name="PSM4-Pull"
        template="Windows Server 2012 R2 PSv5"
        computername="Pull"
        memorystartupbytes="2GB"
        dynamicmemoryenabled="N">
      <dsc configname="MEMBER_DEFAULT"
           configfile="MEMBER_DEFAULT.DSC.ps1">
        <parameters>
          DomainName = "GLOBOMANTICS.COM"
          DomainAdminPassword = "P@ssw0rd"
          DCName = 'DC'
          PSDscAllowDomainUser = $True
        </parameters>
      </dsc>           
      <adapters>
        <adapter name="Internal"
                 switchname="Internal">
        </adapter>
      </adapters>              
    </vm>
    <vm name="PSM4-Win10"
        template="Windows 10 Enterprise"
        computername="Win10"
        dynamicmemoryenabled="N"
        administratorpassword="P@ssw0rd">
    <adapters>
        <adapter name="Internal"
                 switchname="Internal">
        </adapter>
      </adapters>             
   </vm>      
